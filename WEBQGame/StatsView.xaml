<Window x:Class="WEBQGame.StatsView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Статистика" Height="600" Width="650"
        ResizeMode="NoResize"
        Loaded="Window_Loaded">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="370*"/>
            <RowDefinition Height="30*"/>
        </Grid.RowDefinitions>
        <WebBrowser Name="StatsBrowser" AllowDrop="False" Visibility="Visible"></WebBrowser>
        <Button Name="CloseError" Content="Закрыть" Height="23" Width="150" Click="CloseErrorWindow"  Grid.Row="1"/>
    </Grid>
</Window>
